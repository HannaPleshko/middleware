{
  "name": "ews-middleware",
  "version": "1.0.0",
  "description": "EWS Middleware",
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "ISC",
  "keywords": [
    "EWS",
    "Middleware"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepare": "husky install",
    "postinstall": "node_modules/.bin/patch-package",
    "copy:locales": "cp -r ./locales ./dist",
    "build:noemit": "lb-tsc --noEmit --incremental false",
    "build": "lb-tsc && npm run copy:locales",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "lb-eslint --report-unused-disable-directives .",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged --verbose",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "start:dev": "env NODE_ENV=development npm start",
    "start:watch": "nodemon",
    "pretest": "npm run clean && npm run build",
    "test": "npm-run-all test:* --continue-on-error",
    "test:unit": "env TS_NODE_FILES=true mocha -r ts-node/register 'src/tests/**/*.ts'",
    "test:main": "newman run test/ews.postman_collection.json -e test/ews.postman_environment.json --delay-request 5 --insecure",
    "test:messages": "newman run test/ews.message.tests.postman_collection.json -e test/ews.postman_environment.json --delay-request 5 --insecure",
    "test:attachments": "newman run test/ews.attachment.tests.postman_collection.json -e test/ews.postman_environment.json --delay-request 5 --insecure",
    "test:folders": "newman run test/ews.folders_postman_collection.json -e test/ews.postman_environment.json --delay-request 160 --insecure",
    "test:foldersDelegate": "newman run test/ews.foldersDelegate.postman_collection.json -e test/ews.postman_environment.json --delay-request 160 --insecure",
    "test:calendar": "newman run test/ews.calendar_postman_collection.json -e test/ews.postman_environment.json --delay-request 5 --insecure",
    "test:contacts": "newman run test/ews.contacts.tests.postman_collection.json -e test/ews.postman_environment.json --delay-request 5 --insecure",
    "test:tasks": "newman run test/ews.tasks.tests.postman_collection.json -e test/ews.postman_environment.json --delay-request 5 --insecure",
    "test:notes": "newman run test/ews.notes.tests.postman_collection.json -e test/ews.postman_environment.json --delay-request 5 --insecure",
    "test:rules": "newman run test/ews.rules.postman_collection.json -e test/ews.postman_environment.json --delay-request 5 --insecure",
    "combineTests": "node_modules/.bin/postman-combine-collections -f \"test/ews*collection.json\" -n \"EWS Tests\" -o ./reports/ews.collection.json",
    "viewPRTestReport": "open ./reports/ews_postman_report.html",
    "testPR": "npm-run-all combineTests testPR:all viewPRTestReport --continue-on-error",
    "testPR:all": "newman run reports/ews.collection.json -e test/ews.postman_environment.json -r htmlextra --reporter-htmlextra-export ./reports/ews_postman_report.html --reporter-htmlextra-title \"EWS Postman Report\" --delay-request 160 --insecure",
    "precoverage": "lb-nyc npm run test:unit",
    "coverage": "open coverage/lcov-report/index.html",
    "docker:build": "DOCKER_BUILDKIT=1 docker build --no-cache --progress=plain --secret id=npmrcpat,src=$HOME/.npmrc -f ./deploy/dockerfile -t hcllabs/ews .",
    "docker:push": "docker tag hcllabs/ews docker.hcllabs.net/ews:latest & docker push docker.hcllabs.net/ews:latest"
  },
  "dependencies": {
    "@hcllabs/openclientkeepcomponent": "^1.0.92",
    "@loopback/boot": "^2.0.4",
    "@loopback/context": "^3.4.0",
    "@loopback/core": "^2.3.0",
    "@loopback/repository": "^2.1.1",
    "@loopback/rest": "^3.2.1",
    "@loopback/service-proxy": "^2.2.0",
    "@types/luxon": "^1.24.3",
    "@types/xml-formatter": "^1.1.0",
    "accepts": "^1.3.7",
    "email-addresses": "^3.1.0",
    "fast-copy": "^2.0.5",
    "html-to-text": "^5.1.1",
    "i18n": "^0.13.3",
    "is-class": "0.0.9",
    "json-pointer": "^0.6.1",
    "lodash": "^4.17.15",
    "loopback-connector-rest": "^3.7.0",
    "luxon": "^1.25.0",
    "mailparser": "^3.3.1",
    "mimemessage": "^1.0.5",
    "moment-timezone": "^0.5.32",
    "patch-package": "^6.2.1",
    "rrule": "^2.6.6",
    "strong-soap": "2.0.0",
    "tslib": "^1.10.0",
    "uuid": "^7.0.3",
    "windows-iana": "^5.0.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2",
    "xml-formatter": "^2.0.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@loopback/build": "^5.0.1",
    "@loopback/eslint-config": "^6.0.3",
    "@loopback/testlab": "^3.2.9",
    "@types/accepts": "^1.3.5",
    "@types/html-to-text": "^5.1.1",
    "@types/i18n": "^0.13.1",
    "@types/json-pointer": "^1.0.31",
    "@types/lodash": "^4.14.149",
    "@types/mailparser": "^2.7.2",
    "@types/mocha": "^8.0.4",
    "@types/node": "^10.17.19",
    "@types/uuid": "^7.0.2",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "dotenv": "^10.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.2.3",
    "mocha": "^8.1.3",
    "newman": "~5.1.0",
    "newman-reporter-htmlextra": "^1.19.6",
    "nodemon": "^2.0.2",
    "npm-run-all": "4.1.5",
    "postman-combine-collections": "^1.1.1",
    "source-map-support": "^0.5.16",
    "ts-node": "^9.0.0",
    "typescript": "~3.8.3"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "src/"
    ],
    "ignore": [
      "dist/*"
    ],
    "ext": "ts",
    "exec": "npm start"
  },
  "lint-staged": {
    "*.{js,ts}": "lb-eslint --report-unused-disable-directives"
  }
}
